{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "adodb/adodb-php": "^5.20",
        "beyondcode/laravel-websockets": "^1.6",
        "decidir2/php-sdk": "^1.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "infobip/infobip-api-php-client": "^2.3",
        "intervention/image": "^2.7",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "lkaybob/php-fcm-v1": "^1.0.1",
        "phpmailer/phpmailer": "dev-master",
        "phpoffice/phpexcel": "^1.8",
        "web-token/jwt-core": "v2.2.7",
        "web-token/jwt-easy": "v2.2.7",
        "web-token/jwt-encryption-algorithm-aeskw": "v2.2.7",
        "web-token/jwt-signature-algorithm-hmac": "v2.2.7",
        "web-token/jwt-signature-algorithm-rsa": "v2.2.7"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
