{
    "name": "archivosedi/archivosedi",
    "description": "api para generacion y administracion de contenedores",
    "type": "archivos-edi",
    "authors": [
        {
            "name": "Ariel Dupuy",
            "email": "adupuy@cobrodigital.com"
        }
    ],
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "^7.2.5",
        "adodb/adodb-php": "^5.20",
        "beyondcode/laravel-websockets": "^1.6",
        "decidir2/php-sdk": "^1.5",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "infobip/infobip-api-php-client": "^2.3",
        "intervention/image": "^2.7",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "lkaybob/php-fcm-v1": "^1.0.1",
        "phpmailer/phpmailer": "dev-master",
        "phpoffice/phpexcel": "^1.8",
        "web-token/jwt-core": "^2.2",
        "web-token/jwt-easy": "^2.2",
        "web-token/jwt-encryption-algorithm-aeskw": "v2.2.7",
        "web-token/jwt-signature-algorithm-hmac": "v2.2.7",
        "web-token/jwt-signature-algorithm-rsa": "v2.2.7",
        "php-edifact/edifact-generator": "^1.1",
        "ext-bcmath": "*"
        
    },
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:php-edifact/edifact-generator.git"
        }
    ]
    ,
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "sabas/edifact": "0.6.0"
    },
     "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Http\\": "app/Http/",
            "App\\Http\\Controllers\\": "app/Http/Controllers",
            "App\\Http\\Controllers\\Edisoft\\": "app/Http/Controllers/Edisoft/",
            "App\\Http\\Controllers\\Backoffice\\": "app/Http/Controllers/Backoffice/",
            "Traits\\":"traits/"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }

}
